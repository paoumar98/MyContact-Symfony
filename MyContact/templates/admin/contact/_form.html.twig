
{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.prenom) }}</div>
    <div class="col-md-4">{{ form_row(form.nom) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.numero) }}</div>
    <div class="col-md-4">{{ form_row(form.email) }}</div>
</div>

<div class="row">
    <div class="col-md-4">{{ form_row(form.imageFile) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.status) }}</div>
</div>

{{ form_widget(form) }}
<button class="btn btn-primary">{{ button |default('Enregistrer') }}</button>
{{ form_end(form) }}